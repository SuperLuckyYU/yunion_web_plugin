{
	"name": "yunion",
	"displayName": "yunion-diy",
	"description": "云联壹云开发者定制vscode插件",
	"version": "0.0.4",
	"publisher": "Yunion",
	"repository": {
		"type": "git",
		"url": "https://github.com/SuperLuckyYU/yunion_web_plugin.git"
	},
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld"
	],
	"main": "./extension.js",
	"contributes": {
		"snippets": [
			{
				"language": "javascript",
				"path": "./snippets/javascript.json"
			},
			{
				"language": "typescript",
				"path": "./snippets/javascript.json"
			},
			{
				"language": "vue",
				"path": "./snippets/javascript.json"
			}
		]
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^6.6.0",
		"glob": "^7.1.5",
		"mocha": "^10.2.0",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
